{
  "name": "csv-es",
  "version": "1.0.1",
  "description": "A modern, fast, RFC 4180 compliant parser for JS",
  "keywords": [
    "csv",
    "parse",
    "stringify",
    "vanilla",
    "rfc4180"
  ],
  "license": "MIT",
  "contributors": [
    {
      "name": "Evan Plaice",
      "email": "evan.plaice@gmail.com",
      "url": "http://evanplaice.com"
    },
    {
      "name": "Colton Ehrman",
      "email": "coltonje95@gmail.com",
      "url": "http://coltonehrman.com"
    }
  ],
  "repository": "http://github.com/vanillaes/csv-es/",
  "main": "src/csv.js",
  "type": "module",
  "scripts": {
    "test": "tape-es",
    "test:watch": "tape-watch-es",
    "lint": "semistandard",
    "types": "npx tsc -p .config/jsconfig.json",
    "build": "npm run build:cjs && npm run build:typings",
    "build:cjs": "npx rollup --config .config/cjs.config.js",
    "build:typings": "npx tsc -p .config/tsconfig.json",
    "package": "npx rimraf package && npm pack | tail -n 1 | xargs tar -xf",
    "preversion": "npm test && npm run lint && npm run types",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "semistandard": "^14.2.0",
    "tape-es": "^1.1.1"
  },
  "semistandard": {
    "parser": "babel-eslint"
  }
}
